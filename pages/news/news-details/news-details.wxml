<!--pages/news/news-details/news-details.wxml-->
<view class="page" >
  <view class="page__hd">
    <text class="page__title">{{title}}</text>
   
  </view >
  <view class='page_d'> <text class='page_date'>{{date}}</text>
  
  </view>
    <view class="news-details-content wxParse">
    <image src='{{image_url}}'></image>
    <text>{{content}}</text>
    <template is="wxParse" data="{{wxParseData:content.nodes}}"/>
    </view>
</view>
